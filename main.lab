   433 				.macro blit_tile
   434 					lda (map_ptr),y			; Load the tile from the map
   435 					asl						; Multiply by two to get left character
   436 					fix_color				; Fix the color if needed
   437 					sta (screen_ptr),y		; Store the left character
   438 					inc16 screen_ptr		; Advance the screen pointer
   439 					lda (map_ptr),y			; Load the tile from the map
   440 					asl						; Multiply by two to get left character
   441 					add #1					; Add one to get right character
   442 					fix_color				; Fix the color if needed
   443 					sta (screen_ptr),y		; Store the right character
   444 					adw map_ptr #1			; Advance the map pointer
   445 					adw screen_ptr #1		; Advance the screen pointer	
   446 					.endm
