   255 				.macro blit_tile
   256 					lda (map_ptr),y			; Load the tile from the map
   257 					asl						; Multiply by two to get left character
   258 					sta (screen_ptr),y		; Store the left character
   259 					inc16 screen_ptr		; Advance the screen pointer
   260 					add #1					; Add one to get right character
   261 					sta (screen_ptr),y		; Store the right character
   262 					adw map_ptr #1			; Advance the map pointer
   263 					adw screen_ptr #1		; Advance the screen pointer	
   264 					.endm
