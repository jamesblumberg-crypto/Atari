   240 				.macro blit_tile
   241 					lda (map_ptr),y			; Load the tile from the map
   242 					asl						; Multiply by two to get left character
   243 					sta (screen_ptr),y		; Store the left character
   244 					inc16 screen_ptr		; Advance the screen pointer
   245 					add #1					; Add one to get right character
   246 					sta (screen_ptr),y		; Store the right character
   247 					adw map_ptr #1			; Advance the map pointer
   248 					adw screen_ptr #1		; Advance the screen pointer	
   249 					.endm
