   396 				* Proc: fix_color                         *
   397 				* Fixes colors for a character if needed  *
   398 				* --------------------------------------- *
   399 				.macro fix_color
   400 					; A = character index to fix color if needed
   401 					sta tmp					; Save character to temp var
   402 					tya
   403 					pha
   404 					lda tmp					; Reload the character
   405 					and #$07				; Get last 3 bits
   406 					add #1					; Add 1 so that we can shift correct number of times
   407 					sta tmp2				; Store bit shift amount to tmp2
   408 					lda tmp					; Reload char
   409 					lsr						; Divide by 8 to get color index
   410 					lsr						; 
   411 					lsr						; 
   412 					tay					; Store into x to be able to use as an offset
   413 					lda (char_colors_ptr),y	; Get the color bits for this character
   414 				
   415 				shift_bits
   416 					lsr						; Shift until bit is in the carry flag
   417 					dec tmp2				; Reduce shift counter
   418 					bne shift_bits			; Keep looping if shift counter isn't 0
   419 				
   420 					bcc done				; If the carry flag is *not* set, we're done
   421 				
   422 				add_color
   423 					lda tmp					; Load character back into A
   424 					add #128				; Add 128 to change to secondary color
   425 					sta tmp					; Re-save back to tmp after adding
   426 				
   427 				done
   428 					pla						; Pull old X from the stack via A
   429 					tay
   430 					lda tmp					; Re-load character to A
   431 					.endm
