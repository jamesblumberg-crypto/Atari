   510 B3C9 A5 E8			lda player_xp
   511 B3CB C9 5A			cmp #90                    ; Check if XP >= 90
   512 B3CD 90 24			bcc no_level_up            ; If less than 90, no level up
   513
   514 B3CF			level_up
   515 					; Subtract 90 from XP (carry over excess)
   516 B3CF A5 E8			lda player_xp
   517 B3D1 38				sec
   518 B3D2 E9 5A			sbc #90
   519 B3D4 85 E8			sta player_xp
   520
   521 					; Increment level
   522 B3D6 E6 E9			inc player_level
   523
   524 					; Restore HP to max
   525 B3D8 A5 E7			lda player_max_hp
   526 B3DA 85 E6			sta player_hp
